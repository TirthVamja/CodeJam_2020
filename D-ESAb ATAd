#include<bits/stdc++.h>
using namespace std;
void bomb(string& s){
    for(char& x:s){
        if(x=='1')x='0';
        else if(x=='0')x='1';
    }
}
int main()
{
    int t,n;cin>>t>>n;
    while(t--){
        int r=-1,c=-1,i,j;
        char l,ok;
        string st(n,'?');
        for(i=1,j=0;j<n/2;i+=2){if(i>10 && i%10==1){if(c!=-1){
                cout<<c+1<<endl; cin>>l;
                if(st[c]!=l) bomb(st);}
                else {cout<<"1\n";cin>>l;}
                if(r!=-1){
                cout<<r+1<<endl;  cin>>l;
                if(st[r]!=l)reverse(st.begin(),st.end());}
                else {cout<<"1\n";cin>>l;}}
        else{
            cout<<j+1<<endl;  cin >> st[j];
            cout << n-j << endl;  cin >> st[n-1-j];
            if(st[j]==st[n-1-j])c=j;
            else if(st[j]!=st[n-1-j])r=j;
            ++j;}}
        cout<<st<<endl;
        cin>>ok;
        if(ok=='N')return 0;
    }
}
